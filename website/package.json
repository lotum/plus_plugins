{
  "name": "website",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:linting": "eslint . --max-warnings=0",
    "check:formatting": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:typescript": "tsc",
    "check:spelling": "./node_modules/.bin/spellchecker --quiet --files=\"docs/**/*.{md,mdx}\" --dictionaries=\"./dictionary.js\" --reports=\"./spelling.json\" --plugins spell indefinite-article repeated-words syntax-mentions syntax-urls frontmatter"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/plugin-google-gtag": "^2.3.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@types/lodash.get": "^4.4.7",
    "@types/react": "^17.0.50",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.8",
    "classnames": "^2.3.2",
    "compare-versions": "^5.0.3",
    "docusaurus-plugin-sass": "^0.2.3",
    "dotenv": "^16.0.3",
    "lodash.get": "^4.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.9.0",
    "sass": "^1.61.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.3.1",
    "@tsconfig/docusaurus": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "spellchecker-cli": "^6.1.1",
    "typescript": "^5.0.4"
  }
}
